{
  "name": "developeur-meko-lead-react",
  "version": "1.0.0",
  "engines": {
    "npm": ">=3",
    "node": ">=6"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "analyze:clean": "rimraf stats.json",
    "dev": "node server.js",
    "prod": "cross-env NODE_ENV=production node server.js --progress",
    "start-js": "react-scripts start",
    "build": "react-scripts build && npm run build-css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build:staging": "env-cmd .env.staging npm run build",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "watch": "webpack --progress --colors --watch",
    "start": "npm-run-all -p start-js watch-css",
    "monitor": "webpack-dev-server --hot --inline --colors --content-base public/ --host=127.0.0.1",
    "package": "webpack --progress --colors -p",
    "test": "npm run test-enzyme",
    "starts": " jest --coverage && node server.js",
    "test-enzyme": "mocha --compilers js:babel-core/register --require ./test/jsdom-setup.js \"test/enzyme/**/*.@(js|jsx)\"",
    "test-jest": "jest jest --coverage jest --watch",
    "clean": "rimraf ./build/*",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "husky": {
    "hooks": {
      "post-checkout": "npm run generategithash:ignore",
      "pre-commit": "npm run lint && npm run test"
    }
  },
